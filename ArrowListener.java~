/**
 * ArrowListener interface for Tetris project
 */
public interface ArrowListener
{
	void upPressed();
	void downPressed();
	void leftPressed();
	void rightPressed();
	void spacePressed();
	void escPressed();
	void oPressed();
	void pPressed();
	void rPressed();
	void cPressed();
	void mPressed();
	void onePressed();
	void twoPressed();
	void threePressed();
	void fourPressed();
	void sPressed();
	void iPressed();
	void tPressed();
	void dPressed();
}